<a href="/projects/new">Add a new volunteer</a>

<% if @volunteers.any? %>
  <ul>
    <% @volunteers.each do |volunteer| %>
      <li><a href="/volunteers/<%= volunteer.id%>"><%= volunteer.name %></a> </li>
    <% end %>
  </ul>
<% else %>
  <p>There are curently no volunteers to display.</p>
<% end %>
